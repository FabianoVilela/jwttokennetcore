{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationContext": "Server=(localdb)\\mssqllocaldb;Database=JWTAuthenticationCore;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AppSettings": {
    "Secret": "MySecretTokenForAuthentication",
    "ExpireHours": 2,
    "Emitter": "MyApp",
    "ValidateAt": "https://localhost"
  }
}